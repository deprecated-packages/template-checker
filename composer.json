{
    "name": "symplify/template-checker",
    "description": "Check your TWIG templates",
    "license": "MIT",
    "bin": [
        "bin/template-checker"
    ],
    "require": {
        "php": ">=7.2",
        "symfony/dependency-injection": "^5.1",
        "symfony/console": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/yaml": "^4.4|^5.1",
        "nette/utils": "^3.0",
        "nikic/php-parser": "^4.10.2",
        "symplify/symplify-kernel": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.4",
        "latte/latte": "^2.9",
        "nette/application": "^3.0.7"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\TemplateChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\TemplateChecker\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
